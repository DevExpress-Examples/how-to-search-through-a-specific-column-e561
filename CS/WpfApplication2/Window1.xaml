<Window x:Class="WpfApplication2.Window1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" Title="Window1" Height="300" Width="400">
    <Window.Resources>
    </Window.Resources>
    <DockPanel LastChildFill="True">
        <TextBox DockPanel.Dock="Top" Name="autoSearch" TextChanged="autoSearch_TextChanged" />
        <dxg:GridControl x:Name="grid">
            <dxg:GridControl.Columns>
                <dxg:GridColumn FieldName="Text" Width="150" />
                <dxg:GridColumn FieldName="Number">
                </dxg:GridColumn>
            </dxg:GridControl.Columns>
            <dxg:GridControl.View>
                <dxg:TableView x:Name="view" />
            </dxg:GridControl.View>
        </dxg:GridControl>
    </DockPanel>
</Window>
